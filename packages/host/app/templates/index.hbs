{{#unless this.model.isFastBoot}}
  {{page-title 'RuntimeSpike'}}

  <CardCatalogModal />
  <CreateCardModal />

  <button {{on "click" this.start}}>Start</button>
  <button {{on "click" this.stop}}>Stop</button>

  <Go
    @path={{this.model.path}}
    @openFile={{this.model.openFile}}
    @openDirs={{this.model.openDirs}}
    @polling={{this.model.polling}}
  />

  {{outlet}}
{{/unless}}