{
  "name": "@cardstack/boxel-motion",
  "version": "1.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:glint": "glint",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "@glimmer/component": "^1.1.2",
    "ember-auto-import": "^2.4.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-typescript": "^5.2.1",
    "ember-concurrency": "github:machty/ember-concurrency#c6ed562",
    "ember-modifier": "^3.2.7",
    "macro-decorators": "^0.1.2",
    "style-value-types": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@glint/core": "^1.0.0",
    "@glint/environment-ember-template-imports": "^1.0.0",
    "@glint/template": "^1.0.0",
    "@types/htmlbars-inline-precompile": "^3.0.0",
    "@types/qunit": "^2.11.3",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "ember-template-imports": "^3.0.1",
    "ember-template-lint": "^4.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.5.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "typescript": "^5.1.6",
    "webpack": "^5.78.0"
  },
  "peerDependencies": {
    "@glint/environment-ember-loose": "^1.0.0",
    "ember-source": "^3.8 || 4"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {}
}
